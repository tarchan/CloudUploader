<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="320.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.mac.tarchan.clouduploader.CloudUploaderController">
  <children>
    <TextField fx:id="emailField" layoutX="60.0" layoutY="36.0" prefWidth="200.0" />
    <TextField fx:id="passField" layoutX="60.0" layoutY="73.0" prefWidth="200.0" />
    <Button fx:id="loginButton" layoutX="132.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleLoginAction" text="ログイン" />
    <Button fx:id="uploadButton" layoutX="120.0" layoutY="134.0" mnemonicParsing="false" onAction="#handleUploadAction" text="アップロード..." />
  </children>
</AnchorPane>
